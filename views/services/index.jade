extends ../layout

block content
	.back
		section.container
			- each service, i in services
				article.row.services_module
					div.col.s12
						h2.title.bold #{service.title}
						.desc !{service.content}
						ul.list_services 
							if service.serviceList.length > 1
								- each item, i in service.serviceList
									li
										= item
